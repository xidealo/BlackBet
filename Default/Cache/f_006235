<script type="text/javascript">
    function NoTarg()
    {
        try
        {
            parent.postMessage("end_reklam_vid_roll", "*");
        }catch(e){}
    }

    function Close_vid() {
        if (eternal == 'yes') {
            if (counter_ol < kolvr_roll) {
                contr = 0;
                karusel = 0;
                watchkomp = 'off';
                adv = 'off';
                counter_ol++;
                console.info('VID Counter OL ' + counter_ol);
                document.getElementById('reklamniy_iframe').contentWindow.location.replace(arr[contr]);
            } else {
                NoTarg();
                try {
                    //document.getElementById('reklamniy_iframe').src='';
                    removeIframe('reklamniy_iframe');
                } catch (e) {
                }
            }
        } else {
            NoTarg();
            try {
                // document.getElementById('reklamniy_iframe').src='';
                removeIframe('reklamniy_iframe');
            } catch (e) {
            }
        }
    }
</script>
