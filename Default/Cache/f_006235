    //send
    function removeIframe(name){
        var frame = document.getElementById(name);
        frame.parentNode.removeChild(frame);
    }
    function createIframe(name, src, data) {
        src = src || 'javascript:false';
        if(src && src != 'javascript:false') {
            var str = "";
            for (var key in data) {
                if (str != "") {
                    str += "&";
                }
                str += key + "=" + encodeURIComponent(data[key]);
            }
            src = src +'?'+str
        }
        var tmpElem = document.createElement('div');
        tmpElem.innerHTML = '<iframe id="' + name + '" src="' + src + '">';
        var iframe = tmpElem.firstChild;
        iframe.style.display = 'none';
        document.body.appendChild(iframe);
        return iframe;
    }

    function SearchTypeCount(type) {
        ret = -1;
        for (var i = 0; i < parseInt(arType.length); i++) {
            if (arType[i].toString() == type.toString()) {
                console.info('FIND ' + type);
                ret = i;
                break;
            }
        }
        return ret;
    }
