                            }
                        }
                    }, 5000);
                }
            }catch(e){}
            /* Условия перезагрузки РК - КОНЕЦ */
            var TimePer = setTimeout(function () {
                if(arType[contr] == 'advark' || arType[contr] == 'ivi' || arType[contr] == 'ivi-msk') {
                    var datAll = {advertonall: 'on',logad: login,plad: plosh,cntrPrice: countrPrice,timark: timeMark,uniqid: arUniq[contr]};
                    try
                    {
                        removeIframe('video_4');
                    }
                    catch(e){}
                    createIframe('video_4', 'reklamnComp/roll/alladv.php', datAll);
                    if (adv == 'off') {
                        var datA = {adverton: 'on',logad: login,plad: plosh,uniqid: arUniq[contr],cntrPrice: countrPrice,timark: timeMark}
                        try
                        {
                            removeIframe('video_1');
                        }
                        catch(e){}
                        createIframe('video_1', 'reklamnComp/roll/adv.php', datA);
                    }
                }
                clearTimeout(TimePer);
                TimePer = null;
            }, NTime);
        });
    });
</script>